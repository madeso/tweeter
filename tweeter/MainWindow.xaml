<mui:ModernWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:tweeter" x:Class="tweeter.MainWindow"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="Tweeter" IsTitleVisible="True" Height="350" Width="525" ContentSource="Pages/main.xaml">

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="group 1" >
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="link 1" />
                <mui:Link DisplayName="link 2" />
                <mui:Link DisplayName="link 3" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
    
    <mui:ModernWindow.DataContext>
        <local:TweeterView/>
    </mui:ModernWindow.DataContext>
</mui:ModernWindow>
